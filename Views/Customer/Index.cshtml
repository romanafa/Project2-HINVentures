@model dynamic

@{

    ViewBag.Title = "title";
}

<link href="https://fonts.googleapis.com/css2?family=Material+Icons"
      rel="stylesheet">

<div class="menu-container">
    <div class="images-container" onclick="navigateToAssignment()">
        <img src="/images/oppdrag.jpg" width="300px" height="250px" alt="nytt oppdrag" />
        <div class="dashboard-top">
            <i class="material-icons">add_circle</i>
            NYTT OPPDRAG
        </div>
    </div>


    <div class="images-container" onclick="navigateToDevelopers()">
        <img src="/images/bla gjennom utviklerne.jpg" width="300px" height="250px" alt="bla gjennom utviklerne" />
        <div class="dashboard-top">
            <i class="material-icons">groups</i>
            BLA GJENNOM UTVIKLERNE
        </div>
    </div>

    <div class="images-container" onclick="navigateToAssignmentStatus()">
        <img src="/images/status på oppdrag.jpg" width="300px" height="250px" alt="status på oppdrag" />
        <div class="dashboard-top">
            <i class="material-icons">assignment</i>
            STATUS PÅ OPPDRAG
        </div>
    </div>
</div>

<div class="menu-container">
    <div class="images-container" onclick="navigateToAssignment()">
        <img src="/images/betalinger.jpg" width="300px" height="250px" alt="betalinger" />
        <div class="dashboard-bottom">
            <i class="material-icons">payments</i>
            BETALINGER
        </div>
    </div>

    <div class="images-container">
        <img src="/images/send melding.jpg" width="300px" height="250px" alt="send melding" />
        <div class="dashboard-bottom">
            <i class="material-icons">mail</i>
            SEND MELDING
        </div>
    </div>


    <div class="images-container">
        <img src="/images/om hin-ventures.jpg" width="300px" height="250px" alt="om hin-ventures" />
        <div class="dashboard-bottom">
            <i class="material-icons">info</i>
            OM HIN-VENTURES
        </div>
    </div>
</div>

<script type="text/javascript">

    function navigateToDevelopers() {
        window.location.href = "/customer/developers";
    }
    function navigateToAssignmentStatus() {
        window.location.href = "/customer/assignmentStatus";
    }
    function navigateToAssignment() {
        window.location.href = "/customer/NewAssignment";
    }
    function navigateToPayment() {
        window.location.href = "/payment/_CustomerPaymentIndex";
    }
</script>

